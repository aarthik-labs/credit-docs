{
  "name": "credit-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "types:check": "fumadocs-mdx && next typegen && tsc --noEmit",
    "postinstall": "fumadocs-mdx",
    "lint": "eslint",
    "vcs:id:aarthikbandhu": "git config user.name 'AarthikBandhu' && git config user.email 'harshit@aarthiklabs.com'",
    "utilities:copy-codebase": "zsh -lc 'DIR=\"${1:-.}\"; find \"$DIR\" -type f -not -path \"*/.*\" -not -path \"*/src/lib/components/*\" -not -path \"*/src/lib/database/diagrams/*\" -not -path \"*/node_modules/*\" -not -path \"*/dist/*\" -not -path \"*/build/*\" -not -path \"*/public/*\" -not -path \"*/.git/*\" -not -path \"*/.next/*\" -not -name \"pnpm-lock.yaml\" -not -name \"create-commit.sh\" -not -name \"create-commit-message.sh\" -not -name \"AGENTS.md\" -not -name \"README.md\" -not -name \".gitignore\" -not -name \".env\" -not \\( -name \"*.mmd\" -o -name \"*.svg\" -o -name \"*.ico\" -o -name \"*.png\" -o -name \"*.jpg\" -o -name \"*.jpeg\" \\) -exec tail -n +1 {} + | pbcopy' --"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.89.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fumadocs-core": "16.3.1",
    "fumadocs-mdx": "14.1.1",
    "fumadocs-ui": "16.3.1",
    "lucide-react": "^0.561.0",
    "mermaid": "^11.12.2",
    "next": "16.0.10",
    "next-themes": "^0.4.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/mdx": "^2.0.13",
    "@types/node": "^24.10.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.0.10",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  }
}